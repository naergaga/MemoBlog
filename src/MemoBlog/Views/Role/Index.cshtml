@model List<AppRole>

@{
    ViewBag.Title = "Index";
}
<a asp-action="Add">添加</a>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>NormalizedName</th>
            <th>ConcurrencyStamp</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.NormalizedName</td>
                <td>@item.ConcurrencyStamp</td>
                <td>
                    <a asp-action="Edit" asp-route-name="@item.Name">修改</a>
                    <a asp-action="Delete" asp-route-name="@item.Name">删除</a>
                </td>
            </tr>
        }
    </tbody>
</table>